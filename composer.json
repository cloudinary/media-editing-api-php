{
    "name": "cloudinary/media-editing-api-php",
    "description": "Cloudinary Media Editing API",
    "keywords": [
        "cloudinary",
        "sdk",
        "cloud",
        "image management",
        "cdn"
    ],
    "type": "library",
    "homepage": "https://cloudinary.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Cloudinary",
            "homepage": "https://cloudinary.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "Cloudinary\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Cloudinary\\Test\\" : "test/" }
    }
}
